{
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
    "deno.enable": true,
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "files.associations": {
        "*.njk": "html",
        "*.vto": "vento"
    }
}